{
  "name": "brisk-orm",
  "version": "0.0.7",
  "description": "fast, light-weight, brisk ORM to work in nodejs, support mongodb",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:staged": "jest --bail --findRelatedTests",
    "compile": "ttsc",
    "upversion": "npm version patch",
    "clear": "rimraf ./lib",
    "precompile": "npm run clear",
    "postcompile": "copyfiles -u 1 \"./src/**/*\" -e \"./src/**/*.ts\" \"./lib\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruixiaozi/brisk-orm.git"
  },
  "keywords": [
    "fast",
    "light-weight",
    "brisk",
    "orm",
    "nodejs",
    "mongodb"
  ],
  "author": "ruixiaozi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruixiaozi/brisk-orm/issues"
  },
  "homepage": "https://github.com/ruixiaozi/brisk-orm#readme",
  "dependencies": {
    "brisk-ioc": "0.0.7",
    "brisk-log": "0.0.7",
    "brisk-ts-extends": "0.0.7",
    "lodash": "4.17.21",
    "mysql2": "3.1.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/jest": "29.2.4",
    "@types/lodash": "4.14.194",
    "@types/mysql": "2.15.21",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "copyfiles": "2.4.1",
    "dirlint": "1.0.1",
    "eslint": "8.10.0",
    "eslint-config-brisk": "1.0.3",
    "husky": "7.0.4",
    "jest": "29.3.1",
    "lint-staged": "12.3.4",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ttypescript": "1.5.13",
    "typescript": "4.8"
  },
  "overrides": {
    "@jest/expect-utils": "29.4.1"
  }
}
